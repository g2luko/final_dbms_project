triggers='CREATE DEFINER=`root`@`localhost` TRIGGER all_student_info_after_insert\nAFTER INSERT ON all_student_info\nFOR EACH ROW\nBEGIN\n    IF NEW.sports_tag = \'cricket\' THEN\n        INSERT INTO cricket (id, name, year) VALUES (NEW.id, NEW.name, NEW.year);\n    END IF;\nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER all_student_info_after_insert_kabadi\nAFTER INSERT ON all_student_info\nFOR EACH ROW\nBEGIN\n    IF NEW.sports_tag = \'kabadi\' THEN\n        INSERT INTO kabadi (id, name, year) VALUES (NEW.id, NEW.name, NEW.year);\n    END IF;\nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER all_student_info_after_insert_football\nAFTER INSERT ON all_student_info\nFOR EACH ROW\nBEGIN\n    IF NEW.sports_tag = \'football\' THEN\n        INSERT INTO football (id, name, year) VALUES (NEW.id, NEW.name, NEW.year);\n    END IF;\nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER all_student_info_after_insert_basketball\nAFTER INSERT ON all_student_info\nFOR EACH ROW\nBEGIN\n    IF NEW.sports_tag = \'basketball\' THEN\n        INSERT INTO basketball (id, name, year) VALUES (NEW.id, NEW.name, NEW.year);\n    END IF;\nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER all_student_info_after_insert_khokho\nAFTER INSERT ON all_student_info\nFOR EACH ROW\nBEGIN\n    IF NEW.sports_tag = \'khokho\' THEN\n        INSERT INTO khokho (id, name, year) VALUES (NEW.id, NEW.name, NEW.year);\n    END IF;\nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER all_student_info_after_update\nAFTER UPDATE ON all_student_info\nFOR EACH ROW\nBEGIN\n    IF OLD.sports_tag != NEW.sports_tag THEN\n        \n        CASE OLD.sports_tag\n            WHEN \'cricket\' THEN DELETE FROM cricket WHERE id = OLD.id;\n            WHEN \'kabadi\' THEN DELETE FROM kabadi WHERE id = OLD.id;\n            WHEN \'football\' THEN DELETE FROM football WHERE id = OLD.id;\n            WHEN \'basketball\' THEN DELETE FROM basketball WHERE id = OLD.id;\n            WHEN \'khokho\' THEN DELETE FROM khokho WHERE id = OLD.id;\n            \n        END CASE;\n\n        \n        CASE NEW.sports_tag\n            WHEN \'cricket\' THEN INSERT INTO cricket (id, name, year) VALUES (NEW.id, NEW.name, NEW.year);\n            WHEN \'kabadi\' THEN INSERT INTO kabadi (id, name, year) VALUES (NEW.id, NEW.name, NEW.year);\n            WHEN \'football\' THEN INSERT INTO football (id, name, year) VALUES (NEW.id, NEW.name, NEW.year);\n            WHEN \'basketball\' THEN INSERT INTO basketball (id, name, year) VALUES (NEW.id, NEW.name, NEW.year);\n            WHEN \'khokho\' THEN INSERT INTO khokho (id, name, year) VALUES (NEW.id, NEW.name, NEW.year);\n            \n        END CASE;\n    END IF;\nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER all_student_info_after_delete\nAFTER DELETE ON all_student_info\nFOR EACH ROW\nBEGIN\n    DELETE FROM cricket WHERE id = OLD.id;\n    DELETE FROM kabadi WHERE id = OLD.id;\n    DELETE FROM football WHERE id = OLD.id;\n    DELETE FROM basketball WHERE id = OLD.id;\n    DELETE FROM khokho WHERE id = OLD.id;\n\n    \nEND'
